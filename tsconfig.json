{
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "esnext",
    "target": "esnext",
    "noEmit": true,
    "moduleResolution": "Bundler"
  },
  "include": [
    "src"
  ]
}
